  }

  BLOCK_TEMPLATE end_of_program_2
  {
       Text[G53Y0.]
  }

  BLOCK_TEMPLATE end_of_program_3
  {
       Text[G53]
       Text[Y0.]
  }

  BLOCK_TEMPLATE end_of_program_4
  {
       Text[G49]
  }

  BLOCK_TEMPLATE fifth_axis_clamp
